# CentOS-All-In-One.repo
#
# The mirror contains all files needed to deploy caicloud stack.

[BaseOS]
name=CentOS-7 - Base
baseurl=__download_url__/centos/7
gpgcheck=1
enabled=1
gpgkey=__download_url__/centos/7/RPM-GPG-KEY-CentOS-7

[Glusterfs-Mirror]
name=Glusterfs Mirror Repository
baseurl=__download_url__/gluster/centos/7/
enabled=1
gpgcheck=1
gpgkey=__download_url__/gluster/centos/7/RPM-GPG-KEY-CentOS-SIG-Storage

[CentOS-7-Extras]
name=CentOS 7 Full Mirror Repository
baseurl=__download_url__/extras/centos/7/
enabled=1
gpgcheck=0
